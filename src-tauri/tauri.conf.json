{
	"productName": "innohack",
	"version": "0.0.1",
	"identifier": "com.c0ffee.innohack",
	"build": {
		"beforeDevCommand": "yarn dev",
		"devUrl": "https://innohack.kimjinwoo.me",
		"beforeBuildCommand": "yarn build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "innohack",
				"width": 800,
				"height": 600
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
	}
}
